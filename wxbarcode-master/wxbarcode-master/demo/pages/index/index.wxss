page {
  width: 100%;
  height: 100%;
  background: url('http://bpic.588ku.com/back_pic/05/62/48/425b5310a670340.jpg!r650/fw/800') no-repeat center top;
  background-size: cover;
}
.container {
  display: flex;
  flex-direction: column;
  justify-content:space-between;
  padding:200rpx 0;
  box-sizing:border-box;
}
.container .avator {
  display: flex;
  flex-direction:column;
  align-items: center;
}
.container .avator .avator-cont {
  width:150rpx;
  height:150rpx;
  margin:20rpx;
  border-radius:50%;
}

.container .title {
  display: flex;
  justify-content: center;
  font-size: 28rpx;
  padding-top: 60rpx;
  padding-bottom: 20rpx;
  color: #fff;
}

.container .btn-btn {
  width: 76%;
  color: #fff;
  background:#439057;
  font-size:18px;
  box-sizing:border-box;
  border-radius: 0;
}

